{
  "entryPoints": ["src/index.ts"],
  "out": "docs",
  "exclude": [
    "node_modules/**",
    "lib/**"
  ],
  "tsconfig": "tsconfig.json",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "hideBreadcrumbs": true,
  "hideInPageTOC": true,
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": true
}