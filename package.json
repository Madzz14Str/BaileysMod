{
  "name": "@madzoffc/baileysmod",
  "version": "9.0.0-rc.1",
  "description": "A WebSockets library for interacting with WhatsApp Web, Modification by MadzOFFC",
  "keywords": [
    "whatsapp",
    "automation"
  ],
  "homepage": "https://github.com/Madzz14Str/BaileysMod/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Madzz14Str/BaileysMod.git"
  },
  "license": "MIT",
  "author": "MadzOFFC",
  "main": "./lib/index.js",       
  "types": "./lib/index.d.ts",
  "files": [
    "lib/*",
    "WAProto/*",
    "engine-requirements.js"
  ],
  "scripts": {
    "build:all": "npm run build && npm run build:docs",
    "build:docs": "typedoc",
    "build": "tsc -P tsconfig.json",
    "example": "tsx ./Example/example.ts",
    "gen:protobuf": "sh WAProto/GenerateStatics.sh",
    "lint": "tsc && eslint src --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
