{
  "name": "@madzoffc/baileysmod",
  "version": "10.0.0",
  "description": "A WebSockets library for interacting with WhatsApp Web, Modification by MadzOFFC",
  "keywords": ["whatsapp", "automation"],
  "homepage": "https://github.com/Madzz14Str/BaileysMod/",
  "repository": {
    "type": "git",
