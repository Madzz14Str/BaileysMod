    "sharp": "^0.34.3",
    "tsc-esm-fix": "^3.1.2",
    "tsx": "^4.20.3",
    "typedoc": "^0.27.9",
    "typedoc-plugin-markdown": "4.4.2",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "link-preview-js": "^3.0.0",
    "sharp": "^0.34.3"
