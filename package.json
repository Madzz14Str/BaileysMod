    "async-mutex": "^0.5.0",
    "fflate": "^0.8.2",
    "libsignal": "git+https://github.com/whiskeysockets/libsignal-node",
    "jimp": "^1.6.0",
    "lru-cache": "^11.1.0",
    "music-metadata": "^11.7.0",
    "p-queue": "^9.0.0",
    "pino": "^9.6",
    "protobufjs": "^7.2.4",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
